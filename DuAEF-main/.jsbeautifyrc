{
    "indent_size": 4,
    "beautify.language": {
        "js": {
            "type": [ "javascript", "json", "extendscript" ],
            "filename": [ ".jshintrc", ".jsbeautifyrc" ],
            "ext": [ "js", "json", "jsx", "jsxinc" ]
        }
    },
    "space_in_paren": true,
    "space_after_anon_function": true,
    "end_with_newline": true,
    "editor.formatOnSave": true
}
